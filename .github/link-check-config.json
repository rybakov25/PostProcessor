{
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "{{BASEURL}}/"
    }
  ],
  "ignorePatterns": [
    {
      "pattern": "^https://github.com/.*/issues$"
    },
    {
      "pattern": "^https://dotnet.microsoft.com/"
    }
  ],
  "timeout": "10s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "30s"
}
